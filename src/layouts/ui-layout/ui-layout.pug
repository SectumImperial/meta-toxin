block variables
   -
      const title = "UI page"

doctype html
html(lang="ru")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title #{title} 
    link(rel="icon" type="image/svg" sizes="any" href="../favicon/favicon.svg")
    link(rel="manifest" href="../common/manifest.json")
  body.ui-page
    header.ui-page__header 
      .ui-page__logo 
        a(
          href = "start-page.html",
          class = "ui-page__link",
        )
          img(
            class="ui-page__logo-img",
            alt="Логотип"
            src = "../../layouts/ui-layout/logo/logo.svg"
        )
    block content