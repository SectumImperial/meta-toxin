mixin range-slider(options = {})
  - 
    const {
      rangeClass = '',
      rangeOptions = {},
      name = 'Range slider'
     } = options

  div(
    class = `range-slider _js-range-slider ${rangeClass}` 
    data-rangeOptions = rangeOptions
  )
    .range-slider__fields
      .range-slider__name= name
        input(
          type="text"
          name="range-slider-min"
          class="range-slider__input_min range-slider__input"
          readonly
        )
        input(
          type="text"
          name="range-slider-max"
          class="range-slider__input_max range-slider__input"
          readonly
        )
      .range-slider__slider 
        span.range-slider__toggle.range-slider__toggle_min 
        span.range-slider__progress
        span.range-slider__toggle.range-slider__toggle_max
          
         
        
    
   
    
    